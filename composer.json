{
  "name": "facebook-anonymous-publisher/firewall",
  "description": "Facebook anonymous publisher IP filtering.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "BePsvPT",
      "email": "freedom@csie.io"
    }
  ],
  "require": {
    "php": ">=5.6",
    "doctrine/dbal": "^2.5",
    "geoip2/geoip2": "^2.5",
    "illuminate/database": "^5.1",
    "illuminate/session": "^5.1",
    "illuminate/support": "^5.1",
    "s1lentium/iptools": "^1.1",
    "vectorface/whip": "^0.3"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "^0.4",
    "orchestra/testbench": "3.1.*",
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "FacebookAnonymousPublisher\\Firewall\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
